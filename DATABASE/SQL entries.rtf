\f1\b JOIN AUTHORS TABLE WITH BOOKS TABLE USING AUTHOR_ID FROM AUTHORS TABLE AND AUTHOR_ID FROM BOOKS TABLE
\f0\b0 \
select name,title\
from authors\
join books\
on authors.author_id = books.author_id;\
\

\f1\b QUERY TO SEE HOW MANY BOOKS IN ONE GENRE
\f0\b0 \
select genre_name, count(title)\
from genres\
join books\
on genres.genre_id = books.genre_id\
group by genre_name;}