\f1\b\fs26 \cf8 \cb9 Primary & foreign key
\f0\b0\fs48 \cf0 \cb1 \

\fs24 CREATE TABLE genres (\
  genre_id SERIAL PRIMARY KEY,\
  genre_name TEXT NOT NULL\
);\
 \
-- Table 2: authors\
CREATE TABLE authors (\
  author_id SERIAL PRIMARY KEY,\
  name TEXT NOT NULL,\
  birth_year INT,\
  nationality TEXT\
);\
 \
-- Table 3: shelves\
CREATE TABLE shelves (\
  shelf_id SERIAL PRIMARY KEY,\
  shelf_code TEXT NOT NULL UNIQUE,\
  location_description TEXT\
);\
 \
-- Table 4: books\
CREATE TABLE books (\
  book_id SERIAL PRIMARY KEY,\
  title TEXT NOT NULL,\
  author_id INT REFERENCES authors(author_id) ON DELETE CASCADE,\
  genre_id INT REFERENCES genres(genre_id) ON DELETE SET NULL,\
  shelf_id INT REFERENCES shelves(shelf_id) ON DELETE CASCADE,\
  publish_date DATE,\
  isbn TEXT UNIQUE,\
  copies_available INT DEFAULT 1\
);\
\
